{
  "expo": {
    "name": "frontend",
    "slug": "frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/img/img/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "backgroundColor": "#0C0C0C"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/img/adaptive-icon.png",
        "backgroundColor": "#0C0C0C"
      }
    },
    "web": {
      "favicon": "./assets/img/favicon.png"
    }
  }
}
